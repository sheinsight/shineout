    "prettier": "^1.15.3",
    "prettier-miscellaneous": "^1.5.3",
    "prettier-stylelint": "^0.4.2",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.0",
    "raw-loader": "^0.5.1",
    "react": "^16.8.6",
    "react-color": "^2.14.0",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.3.2",
    "react-markdown": "^3.1.4",
    "react-router-dom": "^4.4.0-beta.6",
    "react-test-renderer": "^16.8.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "stylelint": "^9.10.1",
    "stylelint-config-idiomatic-order": "^6.2.0",
    "ts-loader": "8.2.0",
    "typescript": "4.5.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^0.6.2",
